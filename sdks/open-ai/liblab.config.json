{
  "sdkName": "OpenAI",
  "specFilePath": "https://raw.githubusercontent.com/openai/openai-openapi/master/openapi.yaml",
  "languages": [
    "csharp"
  ],
  // OpenAI uses a bearer token for authentication
  "auth": [
    "bearer"
  ],
  "languageOptions": {
    "csharp": {
      "packageId": "liblab.Examples.OpenAI",
      "sdkVersion": "0.0.1"
    }
  },
  // The OpenAI API spec has some warnings that we can ignore
  "validationsToIgnore": [
    {
      "code": "oas3-valid-schema-example"
    },
    {
      "code": "no-$ref-siblings"
    },
    {
      "code": "operation-description"
    },
    {
      "code": "oas3-unused-component"
    }
  ]
}